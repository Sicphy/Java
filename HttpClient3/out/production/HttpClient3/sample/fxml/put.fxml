<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="25" prefHeight="152.0" prefWidth="291.0" vgap="15" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.PutController">
    <children>
        <Button fx:id="openButton" onAction="#openAction" prefHeight="25.0" prefWidth="49.0" text="Open" GridPane.columnIndex="2" GridPane.rowIndex="2" />
        <Button fx:id="getBackButton" onAction="#getBackAction" prefHeight="27.0" prefWidth="19.0" style="-fx-background-image: url(left-arrow.png); -fx-focus-color: transparent; -fx-faint-focus-color: transparent; -fx-border-color: transparent; -fx-background-color: transparent;" GridPane.rowIndex="0" />
        <Button fx:id="sendPutRequestButton" onAction="#sendPutRequestAction" text="Send" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="3" GridPane.rowSpan="1" />
        <Label fx:id="putLabel" text="Choose file for sending:" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1" GridPane.rowSpan="1" />
        <Label fx:id="getBackLabel" text="Back" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="0" GridPane.rowSpan="1" />
        <TextField fx:id="putTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
